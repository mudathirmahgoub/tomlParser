{
  "lang": {
    "type": "string",
    "prefix": "--",
    "allowedValues": [
      "smtlib2.6",
      "tptp",
      "sygus2"
    ],
    "description": "force input language",
    "defaultValue": "smtlib2.6"
  },
  "output-lang": {
    "type": "string",
    "prefix": "--",
    "allowedValues": [
      "smtlib2.6",
      "tptp",
      "sygus2",
      "ast"
    ],
    "description": "force output language",
    "defaultValue": "auto"
  },
  "tlimit": {
    "type": "int",
    "prefix": "--",
    "min": 1,
    "max": 60000,
    "description": "enable time limiting (give milliseconds)",
    "defaultValue": "30000"
  }
}